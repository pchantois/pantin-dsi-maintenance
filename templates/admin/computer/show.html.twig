{% extends 'base.html.twig' %}

{% block title %}Computer{% endblock %}

{% block body %}
    <h1>Computer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ computer.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ computer.libelle }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ computer.ip }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ computer.type }}</td>
            </tr>
            <tr>
                <th>Os</th>
                <td>{{ computer.os }}</td>
            </tr>
            <tr>
                <th>Environnement</th>
                <td>{{ computer.environnement }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_computer_index') }}">back to list</a>

    <a href="{{ path('admin_computer_edit', {'id': computer.id}) }}">edit</a>

    {{ include('admin_computer/_delete_form.html.twig') }}
{% endblock %}
